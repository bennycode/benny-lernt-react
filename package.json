{
  "dependencies": {
    "@types/hapi": "17.0.18",
    "@types/inert": "5.1.1",
    "@types/react": "16.4.11",
    "@types/react-dom": "16.0.7",
    "axios": "0.18.0",
    "cross-env": "5.2.0",
    "hapi": "17.5.3",
    "inert": "5.1.0",
    "logdown": "3.2.7",
    "prop-types": "15.6.2",
    "react": "16.4.2",
    "react-dom": "16.4.2"
  },
  "devDependencies": {
    "@babel/core": "7.0.0-rc.1",
    "@babel/plugin-proposal-class-properties": "7.0.0-rc.1",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0-rc.1",
    "@babel/preset-react": "7.0.0-rc.1",
    "awesome-typescript-loader": "5.2.0",
    "babel-loader": "8.0.0-beta.4",
    "concurrently": "3.6.1",
    "html-webpack-plugin": "3.2.0",
    "nodemon": "1.18.3",
    "rimraf": "2.6.2",
    "typescript": "3.0.1",
    "webpack": "4.16.5",
    "webpack-cli": "3.1.0",
    "webpack-dev-server": "3.1.5"
  },
  "main": "dist/index.js",
  "name": "benny-lernt-react",
  "private": true,
  "scripts": {
    "clean": "rimraf dist",
    "dev": "yarn clean && yarn dist:backend && concurrently \"yarn watch:backend\" \"nodemon\" \"yarn watch:frontend\"",
    "dev:backend": "yarn dist:backend && concurrently \"yarn watch:backend\" \"cross-env NODE_DEBUG=prefix:* NODE_ENV=development nodemon\"",
    "dist": "yarn clean && yarn dist:backend && yarn dist:frontend",
    "dist:backend": "tsc -p tsconfig.backend.json",
    "dist:frontend": "webpack --mode production",
    "start": "yarn dist && cross-env NODE_DEBUG=prefix:* NODE_ENV=production node dist/index.js",
    "watch:backend": "tsc -w -p tsconfig.backend.json",
    "watch:frontend": "webpack-dev-server"
  },
  "version": "1.0.0"
}
