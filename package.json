{
  "dependencies": {
    "@material-ui/core": "3.6.0",
    "@types/hapi": "17.6.4",
    "@types/inert": "5.1.2",
    "@types/react": "16.7.8",
    "@types/react-dom": "16.0.10",
    "axios": "0.18.0",
    "cross-env": "5.2.0",
    "hapi": "17.8.1",
    "inert": "5.1.2",
    "logdown": "3.2.7",
    "prop-types": "15.6.2",
    "react": "16.6.3",
    "react-dom": "16.6.3"
  },
  "devDependencies": {
    "@babel/core": "7.1.6",
    "@babel/plugin-proposal-class-properties": "7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0",
    "@babel/preset-react": "7.0.0",
    "@babel/preset-typescript": "7.1.0",
    "babel-loader": "8.0.4",
    "concurrently": "4.1.0",
    "html-webpack-plugin": "3.2.0",
    "nodemon": "1.18.7",
    "rimraf": "2.6.2",
    "typescript": "3.1.6",
    "webpack": "4.26.1",
    "webpack-cli": "3.1.2",
    "webpack-dev-server": "3.1.10"
  },
  "main": "dist/index.js",
  "name": "benny-lernt-react",
  "private": true,
  "scripts": {
    "clean": "rimraf dist",
    "dev": "yarn clean && yarn dist:backend && concurrently \"yarn watch:backend\" \"nodemon\" \"yarn watch:frontend\"",
    "dev:backend": "yarn dist:backend && concurrently \"yarn watch:backend\" \"cross-env NODE_DEBUG=prefix:* NODE_ENV=development nodemon\"",
    "dist": "yarn clean && yarn dist:backend && yarn dist:frontend",
    "dist:backend": "tsc -p tsconfig.backend.json",
    "dist:frontend": "webpack --mode production",
    "start": "yarn dist && cross-env NODE_DEBUG=prefix:* NODE_ENV=production node dist/index.js",
    "watch:backend": "tsc -w -p tsconfig.backend.json",
    "watch:frontend": "webpack-dev-server"
  },
  "version": "1.0.0"
}
