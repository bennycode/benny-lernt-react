  "private": true,
  "scripts": {
    "clean": "rimraf dist",
    "dev": "yarn clean && yarn dist:backend && concurrently \"yarn watch:backend\" \"nodemon ./dist/index.js\" \"yarn watch:frontend\"",
    "dev:backend": "yarn dist:backend && concurrently \"yarn watch:backend\" \"cross-env NODE_DEBUG=prefix:* NODE_ENV=development nodemon\"",
    "dist": "yarn clean && yarn dist:backend && yarn dist:frontend",
    "dist:backend": "gulp build:backend",
    "dist:frontend": "webpack --mode production",
    "start": "yarn dist && cross-env NODE_DEBUG=prefix:* NODE_ENV=production node dist/index.js",
    "start:dev": "gulp start:dev",
    "test": "yarn dist && exit 0",
    "test:backend": "ts-node node_modules/jasmine/bin/jasmine --config=jasmine.json",
    "watch:backend": "gulp watch:backend",
    "watch:frontend": "webpack-dev-server"
  },
  "version": "1.0.0"
